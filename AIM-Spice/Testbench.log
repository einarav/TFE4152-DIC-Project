    52:  .param  WL2     = 3     ! M2 NMOS gate width factor
    53:  .param  WL3     = 3     ! M3 PMOS gate width factor, this is also multiplied with convertion from nmos to pmos
    54:  .param  WL4     = 3     ! M4 PMOS gate width factor, this is also multiplied with convertion from nmos to pmos
    55:  
    56:  ! Convertion between NMOS and PMOS
    57:  .param NtoPMOS  = 3
    58:  
    59:  ! Size of sampeling capacitor
    60:  .param cs_size  = 2pf
    61:  
    62:  ! Actual lengths
    63:  .param W1   = {WL1 * L}             ! M1, NMOS
    64:  .param W2   = {WL2 * L}             ! M2, NMOS
    65:  .param W3   = {WL3 * L * NtoPMOS}   ! M3, PMOS
    66:  .param W4   = {WL4 * L * NtoPMOS}   ! M4, PMOS
